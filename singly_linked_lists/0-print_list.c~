#include "lists.h"
/**
 *print_list print a list and h pointer if not null
 *@h:elemnts of list 
 *Return :size of list
 */
size_t print_list(const list_t *h)
{
  int i = 0;
  while(h != NULL)
    {
      if(h->str == Null)
	printf("[0] (nil)\n");
      else
	prinf([%d]%s\n,h->len,h->str);
      i++;
      h = h->next;
    }
  return (i);
}
